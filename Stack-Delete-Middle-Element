// delete Middle element from stack
#include<iostream>
#include<bits/stdc++.h>
using namespace std;
void solve(stack<int> &stack,int count,int size)
{
    // base case
    if(count == size/2){
        stack.pop();
        return ;
    }
    int element = stack.top();
    stack.pop();
    solve(stack,count+1,size);
    stack.push(element);
}
void display(stack<int> st)
{
    for(int i = 0 ; i < )
}
int main()
{
    stack<int> st;
    int count = 0;
    st.push(21);
    st.push(22);
    st.push(23);
    st.push(34);
    st.push(25);
    int N = st.size();
    solve(st,count,N);
    display(st);
}
