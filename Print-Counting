#include<bits/stdc++.h>
using namespace std;
int counting(int n)
{
    if(n == 0){
        return 1;
    }
    else{
        cout<<n<<" ";
        int chottiproblem = counting(n-1);
        int baddiproblem = chottiproblem;
        return baddiproblem;
    }
}
int main()
{
    int n;
    cin>>n;
    counting(n);
}
