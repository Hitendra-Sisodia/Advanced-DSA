#include<bits/stdc++.h>
using namespace std;
// starting from 0 counting
int fibonacci(int n)
{   
    if(n == 0){
        return 0;
    }
    if(n == 1){
        return 1;
    }
    else{
        int baddiproblem = fibonacci(n-1) + fibonacci(n-2);
        return baddiproblem;
    }
}
int main()
{
    int n;
    cin>>n;
    cout<<fibonacci(n);
}
